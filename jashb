	RET
	
	MOVESQ:
	ADD COUNTY, 2
	MOV VERIFICAMOV, 1
	CALL CLS
	RET
PRINTJOGO ENDP

; MOVEALIENDIR PROC

	; PUSH	AX
	; PUSH	BX
	; PUSH	CX
	; PUSH	DX
			
	; INC		NAVEX			
			
	; POP AX
	; POP	BX
	; POP	CX
	; POP	DX

	; RET
; MOVEALIENDIR ENDP

TIROP PROC
	PUSH BX
	PUSH NAVEX
	PUSH CX
	
	MOV BX, NAVEX  ;posicao nave
	ADD BX, 7      ;alinha tiro com bico da nave
	MOV TIROX, BX
	MOV SALVAX, BX
	MOV CX, 186
	MOV TIROY, CX
	PRINTCOISAS TIRO, 15, BX, CX, 2, 6
	CALL DELAY
	PRINTCOISAS TIRO, 0, BX, CX, 2, 6
	
	
	
	MOV VERIFICATIRO, 1
		
		POP CX
		POP NAVEX		
	    POP BX
		RET
TIROP ENDP

DELAY PROC

	PUSH	AX
	PUSH	BX
	PUSH	CX
	PUSH	DX
	
	
 	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H
	
 	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H
	
	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H
	
	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H
	
	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H
	
	MOV     CX, 0
	MOV     DX, 256
	MOV     AH, 86H
	INT     15H

	POP AX
	POP	BX
	POP	CX
	POP	DX
	
	RET
DELAY ENDP

PRINTANAVEE PROC                            ;movimenta nave esquerda
	PRINTNAVE   NAVE, 0, 50, 190, 16, 64
	MOV BX, NAVEX
	ADD BX, 8
	PRINTNAVE   NAVE, 0, BX, 190, 16, 64
	MOV BX, NAVEX
	PRINTNAVE   NAVE, 2, BX, 190, 16, 64	
	
	RET
PRINTANAVEE ENDP

PRINTANAVED PROC                            ;movimenta nave direita
	PRINTNAVE   NAVE, 0, 50, 190, 16, 64
	MOV BX, NAVEX
	SUB BX, 8
	PRINTNAVE   NAVE, 0, BX, 190, 16, 64
	MOV BX, NAVEX
	PRINTNAVE   NAVE, 2, BX, 190, 16, 64	
	
	RET
PRINTANAVED ENDP

