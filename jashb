	MOV ALIENY, 110
	MOV CX, ALIENY
	ADD CX, COUNTY
	MOV ALIENX, 270
	MOV BX, ALIENX
	ADD BX, COUNTX
	PRINTCOISAS	ALIEN33, 15, BX, CX, 16, 128
	
	CMP COUNTX, 0
	JE MOVESQQ
	
	DEC COUNTX
	RET
	
	MOVESQQ:
	ADD COUNTY, 2
	MOV VERIFICAMOV, 0
	CMP COUNTY, 60
	JE LIVEN
	CALL CLS
	RET
	
	LIVEN:
	INC BARREIRACOUNT
	INC COUNTLIVES
	MOV COUNTY, 0
	CALL CLS
	RET
PRINTJOGO2 ENDP

PRINTLIVE3 PROC
	PRINTCOISAS CORACAO, 12, 289, 0, 9, 72
	PRINTCOISAS CORACAO, 12, 299, 0, 9, 72
	PRINTCOISAS CORACAO, 12, 309, 0, 9, 72
	RET
PRINTLIVE3 ENDP

PRINTLIVE2 PROC
	PRINTCOISAS CORACAO, 12, 289, 0, 9, 72
	PRINTCOISAS CORACAO, 12, 299, 0, 9, 72
	RET
PRINTLIVE2 ENDP

PRINTLIVE1 PROC
	PRINTCOISAS CORACAO, 12, 289, 0, 9, 72
	RET
PRINTLIVE1 ENDP

PRINTLIVE0 PROC
	
	RET
PRINTLIVE0 ENDP

PERDEUP PROC
	CALL CLS
	MOV AX, @DATA
	MOV ES, AX
		
	MOV AX, 1300H		;mensagem perdeu
	MOV BX, 2			;COR
	MOV DH, 10 	        ;LINHA
	MOV DL, CENPERDEU 	;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
	MOV CX, LENPERDEU
	LEA BP, PERDEU
	INT 10h

	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 20 	  		  ;LINHA
	MOV DL, CENTRAPRESS   ;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
	MOV CX, LENPRESS
	LEA BP, PRESS
	INT 10h

	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 24 	  		  ;LINHA
	MOV DL, CENFEITO   		  ;COLUNA
	MOV CX, LENFEITO
	LEA BP, FEITO
	INT 10h

	MOV AH, 07h ;pressione qualquer tecla
	INT 21h

	MOV AH, 4CH
	INT 21H
PERDEUP ENDP

; VERIFICABARREIRA PROC

	; CMP COUNTY, 60
	; JE DEGRADA
	; RET

	; DEGRADA:
	; INC BARREIRACOUNT
	
	; RET
; VERIFICABARREIRA ENDP

PRINTABARREIRAS PROC
	CMP BARREIRACOUNT, 0
	JE DEGPT1
	CMP BARREIRACOUNT, 1
	JE DEGPT2
	CMP BARREIRACOUNT, 2
	JE DEGPT3
	CMP BARREIRACOUNT, 3
	JE DEGPT4
	CALL PERDEUP
	
	DEGPT1: JMP DEGRADA1
	DEGPT2: JMP DEGRADA2
	DEGPT3: JMP DEGRADA3
	DEGPT4: JMP DEGRADA4
	 
	DEGRADA1:
		PRINTCOISAS	BARR, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR, 2, 260, 170, 20, 160	
	DEGRADA2:	
		PRINTCOISAS	BARR2, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 260, 170, 20, 160	
	DEGRADA3:	
		PRINTCOISAS	BARR3, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 260, 170, 20, 160		
	DEGRADA4:	
		PRINTCOISAS	BARR4, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 260, 170, 20, 160	
RET
PRINTABARREIRAS ENDP

ATUALIZATIRO PROC		
		PUSH CX  ;SALVA
		CALL DELAY
		POP CX   ;OPEN       
		
		MOV BX, SALVAX
		MOV CX, TIROY
		PRINTCOISAS TIRO, 0, BX, CX, 2, 6

		PUSH CX  ;SALVA
    	CALL DELAY
    	POP CX   ;OPEN 	
    
    	
    	SUB TIROY, 6
		MOV DX, TIROY
		MOV SALVAY, DX
		MOV CX, TIROY
		MOV BX, SALVAX
    	PRINTCOISAS TIRO, 15, BX, CX, 2, 6
    	
    	NXT2:
    	CMP TIROY, 0
    	JE FIMTIRO
		RET
    	
		FIMTIRO:
    	MOV BX, SALVAX
    	PRINTCOISAS TIRO, 0, BX, 0, 2, 6
		MOV VERIFICATIRO, 0
		


	RET
ATUALIZATIRO ENDP

PRINTGANHOU PROC
MOV AX, 1300H		;mensagem ganhou
MOV BX, 2			;COR
MOV DH, 10 	        ;LINHA
MOV DL, CENGANHOU 	;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
MOV CX, LENGANHOU
LEA BP, GANHOU
INT 10h

; MOV AX, 1300H
; MOV BX, 15            ;COR
; MOV DH, 20 	  		  ;LINHA
; MOV DL, CENTRAPRESS   ;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
; MOV CX, LENPRESS
; LEA BP, PRESS
; INT 10h

MOV AX, 1300H
MOV BX, 2            ;COR
MOV DH, 17 	  		  ;LINHA
MOV DL, CENJOGAR   		  ;COLUNA
MOV CX, LENJOGAR
LEA BP, JOGAR
INT 10h

MOV AX, 1300H
MOV BX, 2            ;COR
MOV DH, 24	  		  ;LINHA
MOV DL, CENFEITO   		  ;COLUNA
MOV CX, LENFEITO
LEA BP, FEITO
INT 10h

RET
PRINTGANHOU ENDP

VERIFICAHIT PROC
	MOV BX, XENS1
	CMP SALVAX, BX
	JE PONTECOL1
	INC BX
	CMP SALVAX, BX
	JE PONTECOL1
	INC BX
	CMP SALVAX, BX
	JE PONTECOL1
	INC BX
	CMP SALVAX, BX
	JE PONTECOL1
	INC BX
	CMP SALVAX, BX
	JE PONTECOL1
	INC BX
	CMP SALVAX, BX
	JE PONTECOL1
	INC BX
	CMP SALVAX, BX
	JE PONTECOL1
	INC BX
	CMP SALVAX, BX
	JE PONTECOL1
	
	MOV BX, XENS2
	CMP SALVAX, BX
	JE PONTECOL2
	
	MOV BX, XENS3
	CMP SALVAX, BX
	JE PONTECOL3
	
	MOV BX, XENS4
	CMP SALVAX, BX
	JE PONTECOL4
								PONTECOL1: JMP COLUNA1
								PONTECOL2: JMP COLUNA2
								PONTECOL3: JMP COLUNA3
								PONTECOL4: JMP COLUNA4
								PONTECOL5: JMP COLUNA5
								PONTECOL6: JMP COLUNA6
								PONTECOL7: JMP COLUNA7
								PONTECOL8: JMP COLUNA8
								PONTECOL9: JMP COLUNA9
	MOV BX, XENS5
	CMP SALVAX, BX
	JE PONTECOL5
	
	MOV BX, XENS6
	CMP SALVAX, BX
	JE PONTECOL6
	
	MOV BX, XENS7
	CMP SALVAX, BX
	JE PONTECOL7
	
	MOV BX, XENS8
	CMP SALVAX, BX
	JE PONTECOL8
	
	MOV BX, XENS9
	CMP SALVAX, BX
	JE PONTECOL9
	
	COLUNA1:
	CMP SALVAY, 30
	JE PONTE1
	CMP SALVAY, 50
	JE PONTE10
	CMP SALVAY, 70
	JE PONTE19
	CMP SALVAY, 90
	JE PONTE28
	CMP SALVAY, 110
	JE PONTE37
	
	PONTE1:  JMP ALIEN1HIT
	PONTE10: JMP ALIEN10HIT
	PONTE19: JMP ALIEN19HIT
	PONTE28: JMP ALIEN28HIT
	PONTE37: JMP ALIEN37HIT
	
	COLUNA2:
	CMP SALVAY, 30
	JE PONTE2
	CMP SALVAY, 50
	JE PONTE11
	CMP SALVAY, 70
	JE PONTE20
	CMP SALVAY, 90
	JE PONTE29
	CMP SALVAY, 110
	JE PONTE38
	
	PONTE2:  JMP ALIEN2HIT
	PONTE11: JMP ALIEN11HIT
	PONTE20: JMP ALIEN20HIT
	PONTE29: JMP ALIEN29HIT
	PONTE38: JMP ALIEN38HIT
	
	COLUNA3:
	CMP SALVAY, 30
	JE PONTE3
	CMP SALVAY, 50
	JE PONTE12
	CMP SALVAY, 70
	JE PONTE21
	CMP SALVAY, 90
	JE PONTE30
	CMP SALVAY, 110
	JE PONTE39
	
	PONTE3:  JMP ALIEN3HIT
	PONTE12: JMP ALIEN12HIT
	PONTE21: JMP ALIEN21HIT
	PONTE30: JMP ALIEN30HIT
	PONTE39: JMP ALIEN39HIT
	
	COLUNA4:
	CMP SALVAY, 30
	JE PONTE4
	CMP SALVAY, 50
	JE PONTE13
	CMP SALVAY, 70
	JE PONTE22
	CMP SALVAY, 90
	JE PONTE31
	CMP SALVAY, 110
	JE PONTE40
	
	PONTE4:  JMP ALIEN4HIT
	PONTE13: JMP ALIEN13HIT
	PONTE22: JMP ALIEN22HIT
	PONTE31: JMP ALIEN31HIT
	PONTE40: JMP ALIEN40HIT
	
	COLUNA5:
	CMP SALVAY, 30
	JE PONTE5
	CMP SALVAY, 50
	JE PONTE14
	CMP SALVAY, 70
	JE PONTE23
	CMP SALVAY, 90
	JE PONTE32
	CMP SALVAY, 110
	JE PONTE41
	
	PONTE5:  JMP ALIEN5HIT
	PONTE14: JMP ALIEN14HIT
	PONTE23: JMP ALIEN23HIT
	PONTE32: JMP ALIEN32HIT
	PONTE41: JMP ALIEN41HIT
	
	COLUNA6:
	CMP SALVAY, 30
	JE PONTE6
	CMP SALVAY, 50
	JE PONTE15
	CMP SALVAY, 70
	JE PONTE24
	CMP SALVAY, 90
	JE PONTE33
	CMP SALVAY, 110
	JE PONTE42
	
	PONTE6:  JMP ALIEN6HIT
	PONTE15: JMP ALIEN15HIT
	PONTE24: JMP ALIEN24HIT
	PONTE33: JMP ALIEN33HIT
	PONTE42: JMP ALIEN42HIT
	
	COLUNA7:
