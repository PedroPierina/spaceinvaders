TITLE SQ
.MODEL small
.STACK 100h

PRINTCOISAS	MACRO MATRIZ, COR, XINICIAL, YINICIAL, LARGURA, TOTAL
	PUSH CX
	MOV	X_INICIO,0
	MOV	Y_INICIO, 0
	MOV	TOTALMAT,0
	MOV	LARGURAMAT,0
	XOR	SI,SI

	MOV	AL, COR
	MOV	X_INICIO, XINICIAL
	MOV	Y_INICIO, YINICIAL
	MOV	TOTALMAT, TOTAL
	MOV	LARGURAMAT, LARGURA
	LEA	SI, MATRIZ

	CALL	 PRINTAMAT

	POP 	CX

ENDM

PRINTNAVE  MACRO MATRIZNAVE, COR, XINAVE, YINAVE, LARGURANAVE, TOTALNAVE
	PUSH CX
	MOV	X_INICIO,0
	MOV	Y_INICIO, 0
	MOV	TOTALMAT,0
	MOV	LARGURAMAT,0
	XOR	SI,SI

	MOV	AL, COR
	MOV	X_INICIO, XINAVE
	MOV	Y_INICIO, YINAVE
	MOV	TOTALMAT, TOTALNAVE
	MOV	LARGURAMAT, LARGURANAVE
	LEA	SI, MATRIZNAVE

	CALL	 PRINTAMAT

	POP 	CX

ENDM


.DATA
	SPACE 		DB 'SPACE INVADERS'
	LENESCAPE   EQU $-SPACE
	CENTRASPACE EQU 20 - (LENESCAPE/2)
	
