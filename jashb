	PONTE18: JMP ALIEN18HIT
	PONTE27: JMP ALIEN27HIT
	PONTE36: JMP ALIEN36HIT
	PONTE45: JMP ALIEN45HIT
	
	ALIEN1HIT:
	LEA SI, NAVESLIVES
	MOV BL, [SI]
	MOV BL, 0
	MOV [SI], BL
	RET
	ALIEN2HIT:
	LEA SI, NAVESLIVES
	ADD SI, 1
	MOV BL, [SI]
	MOV BL, 0
	RET
	ALIEN3HIT:
	LEA SI, NAVESLIVES
	ADD SI, 2
	MOV BL, [SI]
	MOV BL, 0
