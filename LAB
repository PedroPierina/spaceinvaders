	MOV BX, ALIENX
	ADD BX, COUNTX
	ADD BX, 1
	PRINTCOISAS	ALIEN11, 0, BX, CX, 16, 128
	MOV ALIENY, 50
	MOV CX, ALIENY
	ADD CX, COUNTY
	MOV ALIENX, 30
	MOV BX, ALIENX
	ADD BX, COUNTX
	ADD BX, 1
