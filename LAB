PERDEUP PROC
	CALL CLS
	MOV AX, @DATA
	MOV ES, AX
		
	MOV AX, 1300H		;mensagem perdeu
	MOV BX, 2			;COR
	MOV DH, 10 	        ;LINHA
	MOV DL, CENPERDEU 	;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
	MOV CX, LENPERDEU
	LEA BP, PERDEU
	INT 10h

	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 20 	  		  ;LINHA
	MOV DL, CENTRAPRESS   ;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
	MOV CX, LENPRESS
	LEA BP, PRESS
	INT 10h

	MOV AX, 1300H
	MOV BX, 2            ;COR
	MOV DH, 24 	  		  ;LINHA
	MOV DL, CENFEITO   		  ;COLUNA
	MOV CX, LENFEITO
	LEA BP, FEITO
	INT 10h

	MOV AH, 07h ;pressione qualquer tecla
	INT 21h

	MOV AH, 4CH
	INT 21H
PERDEUP ENDP

; VERIFICABARREIRA PROC

	; CMP COUNTY, 60
	; JE DEGRADA
	; RET

	; DEGRADA:
	; INC BARREIRACOUNT
	
	; RET
; VERIFICABARREIRA ENDP

PRINTABARREIRAS PROC
	CMP BARREIRACOUNT, 0
	JE DEGPT1
	CMP BARREIRACOUNT, 1
	JE DEGPT2
	CMP BARREIRACOUNT, 2
	JE DEGPT3
	CMP BARREIRACOUNT, 3
	JE DEGPT4
	CALL PERDEUP
	
	DEGPT1: JMP DEGRADA1
	DEGPT2: JMP DEGRADA2
	DEGPT3: JMP DEGRADA3
	DEGPT4: JMP DEGRADA4
	 
	DEGRADA1:
		PRINTCOISAS	BARR, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR, 2, 260, 170, 20, 160	
	DEGRADA2:	
		PRINTCOISAS	BARR2, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR2, 2, 260, 170, 20, 160	
	DEGRADA3:	
		PRINTCOISAS	BARR3, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR3, 2, 260, 170, 20, 160		
	DEGRADA4:	
		PRINTCOISAS	BARR4, 2, 40, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 110, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 190, 170, 20, 160
		PRINTCOISAS	BARR4, 2, 260, 170, 20, 160	
RET
PRINTABARREIRAS ENDP

ATUALIZATIRO PROC		
		PUSH CX  ;SALVA
		CALL DELAY
		POP CX   ;OPEN       
		
		MOV BX, SALVAX
		MOV CX, TIROY
		PRINTCOISAS TIRO, 0, BX, CX, 2, 6

		PUSH CX  ;SALVA
    	CALL DELAY
    	POP CX   ;OPEN 	
    
    	
    	SUB TIROY, 6
		MOV DX, TIROY
		MOV SALVAY, DX
		MOV CX, TIROY
		MOV BX, SALVAX
    	PRINTCOISAS TIRO, 15, BX, CX, 2, 6
    	
    	NXT2:
    	CMP TIROY, 0
    	JE FIMTIRO
		RET
    	
		FIMTIRO:
    	MOV BX, SALVAX
    	PRINTCOISAS TIRO, 0, BX, 0, 2, 6
		MOV VERIFICATIRO, 0
		


	RET
ATUALIZATIRO ENDP

PRINTGANHOU PROC
MOV AX, 1300H		;mensagem ganhou
MOV BX, 2			;COR
MOV DH, 10 	        ;LINHA
MOV DL, CENGANHOU 	;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
MOV CX, LENGANHOU
LEA BP, GANHOU
INT 10h

; MOV AX, 1300H
; MOV BX, 15            ;COR
; MOV DH, 20 	  		  ;LINHA
; MOV DL, CENTRAPRESS   ;COLUNA, vai sempre o centro da tela menos metade da frase, centralizado
; MOV CX, LENPRESS
; LEA BP, PRESS
; INT 10h

MOV AX, 1300H
MOV BX, 2            ;COR
MOV DH, 17 	  		  ;LINHA
MOV DL, CENJOGAR   		  ;COLUNA
MOV CX, LENJOGAR
LEA BP, JOGAR
INT 10h

MOV AX, 1300H
MOV BX, 2            ;COR
MOV DH, 24	  		  ;LINHA
MOV DL, CENFEITO   		  ;COLUNA
MOV CX, LENFEITO
LEA BP, FEITO
INT 10h

RET
PRINTGANHOU ENDP

VERIFICAHIT PROC
	MOV BX, XENS1
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 1
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 2
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 3
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 4
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 5
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 6
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 7
	CMP SALVAX, BX
	JE PONTECOL1
	JMP NECST1
									PONTECOL1: JMP COLUNA1
									NECST1:
	MOV BX, XENS1
	ADD BX, 8
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
	ADD BX, 9
	CMP SALVAX, BX
	JE PONTECOL1
	MOV BX, XENS1
