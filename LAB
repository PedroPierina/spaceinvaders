TIROALIEN PROC
		
	;MOV BX, TIROALX
	;MOV CX, TIROALY
	;MOV ALIENX, BX
	;MOV ALIENY, CX
	
	PUSH CX  ;SALVA
    CALL DELAY
    POP CX   ;OPEN 
	
	MOV BX, XENS1 ; TIROALX  ;posicao DO ALIEN
	ADD BX, 10      ;alinha O TIRO
	MOV TIROX2, BX
	MOV SALVAX2, BX
	MOV CX, YENS5 ;TIROALY
	;MOV TIROY2, CX
	ADD CX, 10
	PRINTCOISAS TIRO, 15, BX, CX, 2, 6
	;CALL DELAY
	PRINTCOISAS TIRO, 0, BX, CX, 2, 6
	
	MOV VERIFICATIROA, 1
	MOV SALVAY2, CX
	
	RET
TIROALIEN ENDP

ATUALIZATIROA PROC
	
	MOV BX, SALVAX2
	MOV CX, SALVAY2
	PRINTCOISAS TIRO, 0, BX, CX, 2, 6

	;PUSH CX  ;SALVA
   	;CALL DELAY
   	;POP CX   ;OPEN 	
   
   	
   	ADD SALVAY2, 3
	MOV CX, SALVAY2
	MOV BX, SALVAX2
   	PRINTCOISAS TIRO, 15, BX, CX, 2, 6
   	
   	NXT22:
   	CMP SALVAY2, 190
   	JA FIMTIRO2
	RET
   	
	FIMTIRO2:
   	MOV BX, SALVAX2
   	PRINTCOISAS TIRO, 0, BX, 192, 2, 6
	MOV VERIFICATIROA, 0
	MOV SALVAX2, 0
	MOV SALVAY2, 0
	
	;ADD COUNTIRO, 1
	ADD TA, 1	
	
	RET
ATUALIZATIROA ENDP

TIROALIEN2 PROC
		
	;MOV BX, TIROALX
	;MOV CX, TIROALY
	;MOV ALIENX, BX
	;MOV ALIENY, CX
	
	PUSH CX  ;SALVA
    CALL DELAY
    POP CX   ;OPEN 
	
	MOV BX, XENS4 ; TIROALX  ;posicao DO ALIEN
	ADD BX, 10      ;alinha O TIRO
	MOV TIROX2, BX
	MOV SALVAX2, BX
	MOV CX, YENS5 ;TIROALY
	;MOV TIROY2, CX
	ADD CX, 10
	PRINTCOISAS TIRO, 15, BX, CX, 2, 6
	;CALL DELAY
	PRINTCOISAS TIRO, 0, BX, CX, 2, 6
	
	MOV VERIFICATIROA, 1
	MOV SALVAY2, CX
	
	RET
TIROALIEN2 ENDP

END
